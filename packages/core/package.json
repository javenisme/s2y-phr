{
  "name": "@medplum/core",
  "version": "4.3.10",
  "description": "Medplum TS/JS Library",
  "keywords": [
    "medplum",
    "fhir",
    "healthcare",
    "interoperability",
    "json",
    "serialization",
    "hl7",
    "standards",
    "clinical",
    "dstu2",
    "stu3",
    "r4",
    "normative"
  ],
  "homepage": "https://www.medplum.com/",
  "bugs": {
    "url": "https://github.com/medplum/medplum/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medplum/medplum.git",
    "directory": "packages/core"
  },
  "license": "Apache-2.0",
  "author": "Medplum <hello@medplum.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.ts", 
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local --config api-extractor.simple.json",
    "api-documenter": "npx --package=@microsoft/api-documenter api-documenter markdown --input-folder ./dist/api/ --output-folder ./dist/docs/",
    "api-extractor": "npx --package=@microsoft/api-extractor api-extractor run --local --config api-extractor.simple.json",
    "build": "npm run clean && node build.mjs",
    "build:full": "npm run clean && node build.mjs && npm run api-extractor && npm run api-documenter",
    "build:esbuild": "npm run clean && npx --package=typescript tsc --project tsconfig.build.json && node esbuild.mjs && npm run api-extractor && npm run api-documenter",
    "clean": "npx --package=rimraf rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "esbuild": "0.25.9"
  },
  "devDependencies": {
    "@medplum/definitions": "4.3.10",
    "@medplum/fhirtypes": "4.3.10",
    "jest-websocket-mock": "2.5.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "pdfmake": "^0.2.5"
  },
  "peerDependenciesMeta": {
    "pdfmake": {
      "optional": true
    }
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
